{
  "$schema": "./node_modules/lerna/schemas/lerna-schema.json",
  "changelog": true,
  "command": {
    "publish": {
      "registry": "https://registry.npmjs.org",
      "allowBranch": [
        "publish",
        "devpublish",
        "devpublish3",
        "devpublishn2",
        "devpublish4",
        "devpublish5"
      ],
      "message": "chore(release): publish",
      "ignoreChanges": [
        "**/__fixtures__/**",
        "**/__tests__/**",
        "**/*.md",
        "**/example/**"
      ]
    },
    "version": {
      "allowBranch": [
        "master",
        "develop",
        "publish",
        "devpublish",
        "devpublish3",
        "devpublishn2",
        "devpublish4",
        "devpublish5"
      ],
      "private": true,
      "conventionalCommits": true,
      "message": "chore: release package(s)"
    }
  },
  "npmClient": "yarn",
  "useNx": true,
  "packages": [
    "packages-modules/**",
    "packages/**",
    "servers/*",
    "portable-devices/*"
  ],
  "version": "5.0.1"
}